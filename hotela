Room: {
	id,
	name,
	type,
	occupied,
	reserved(true when reservation is made false after check in time)
},

RoomType: {
	id,
	name,
	description,
	image,
	price,
	facilities,
}

Facility: {
	id,
	name
}

RoomReservation: {
	id,
	client-name,
	RoomType,
	check-in-date,
	check-out-date,
	reservation-date,
	assured,
	cost
}

